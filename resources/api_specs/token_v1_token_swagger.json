                }
            }
        },
        "v1Token": {
            "description": "Token represents the credentials used to authorize\nthe requests to access protected resources on the OAuth 2.0\nprovider's backend.",
            "type": "object",
            "properties": {
                "applicationName": {
                    "description": "application_name is the full name of the authorizing application",
                    "type": "string",
                    "title": "application_name is the name of the requesting application"
                },
                "accessToken": {
                    "description": "An access token is a string used when making authenticated requests to an API. It represents that\nthe user has authorized a third-party application access to their account",
                    "type": "string",
                    "title": "access_token is the short life access token"
                },
                "refreshToken": {
                    "description": "Refresh tokens are used to request a new access token from an authorization server - in this case\na new access token will be retrieved from Okta on behalf of the user",
                    "type": "string",
                    "title": "refresh_token is a longer-life token used to request additional access tokens"
                },
                "tokenType": {
